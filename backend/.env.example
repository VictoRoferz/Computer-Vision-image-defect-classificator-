# Environment Variables for PCB Labeling Workflow System
# Copy this file to .env and fill in your values

# =============================================================================
# Label Studio Configuration
# =============================================================================

# API Key (get this from Label Studio UI after first login)
# Go to: Account & Settings > Access Token
LABEL_STUDIO_API_KEY=

# Project ID (optional, will be auto-created if not set)
LABEL_STUDIO_PROJECT_ID=

# =============================================================================
# Optional: Label Studio Credentials
# =============================================================================

# Uncomment to set initial credentials
# LABEL_STUDIO_USERNAME=admin
# LABEL_STUDIO_PASSWORD=changeme

# =============================================================================
# PI3 Sender Configuration
# =============================================================================

# Use real camera (true) or sample images (false)
USE_CAMERA=false

# Camera index (if using real camera)
CAMERA_INDEX=0

# =============================================================================
# PI5 Receiver Configuration
# =============================================================================

# Watcher polling interval (seconds)
WATCHER_POLL_INTERVAL=5

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
