# ============================================================
# PCB Defect Classification System - Configuration
# ============================================================

# ============================================================
# General Settings
# ============================================================
LOG_LEVEL=INFO

# ============================================================
# Server 1: Camera Service (Port 8001)
# ============================================================
SERVER1_USE_CAMERA=false
SERVER1_CAMERA_INDEX=0
SERVER1_CAMERA_WIDTH=1920
SERVER1_CAMERA_HEIGHT=1080

# ============================================================
# Server 2: Label Studio Service (Port 8002)
# ============================================================
SERVER2_USE_CONTENT_ADDRESSING=true

# ============================================================
# Label Studio Configuration (Port 8080)
# ============================================================
# IMPORTANT: Add your API key here after first login!
# 1. Open http://localhost:8080
# 2. Login with: admin@example.com / admin123
# 3. Go to Account & Settings > Access Token
# 4. Copy token and paste below:
LABELSTUDIO_API_KEY=

LABELSTUDIO_PROJECT_NAME=PCB Defect Classification
LABELSTUDIO_USERNAME=admin@example.com
LABELSTUDIO_PASSWORD=admin123
